          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="emptystring">
    <xs:restriction base="xs:string">
      <xs:enumeration value="" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="emptyordecimal">
    <xs:union memberTypes="emptystring xs:decimal" />
  </xs:simpleType>
</xs:schema>